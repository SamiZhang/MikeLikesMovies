<div class="group-reviews">

  <img class = "movie-poster"  src= <%= @groupreview.movie.poster_image %> >

    <div class="reviews">
    <% @groupreview.reviews.each do |review| %>
      <h2 ><%= review.title %></h2>
      <p> <%= review.body %>
        <br>
          <span> <%=review.user.email%></span>
      </p>

    <%end%>
  </div>

</div>

